@{
    Layout = null;
    var Keyid = "SchedulPrint";
    List<List<XYJHZX_MVC.Models.SchedulPrint>> Dates = ViewBag.SchedulDate;
}

@if (Dates != null)
{
    <table id="@(Keyid)" class="table table-striped table-bordered" style="font-size:4px;">

        @for (int i = 0; i < Dates.Count; i += 2)
        {
            <tr>
                <td colspan="7">@Dates[i][0].TeamDescption</td>
                <td colspan="7">@((i + 1 < Dates.Count) ? Dates[i + 1][0].TeamDescption : "")</td>
            </tr>
            <tr>
                <td><div style="width:30px;">机号</div></td>
                <td><div style="width:40px;">姓名</div></td>
                <td><div style="width:60px;">透析器</div></td>
                <td><div style="width:60px;">路径</div></td>
                <td><div style="width:60px;">抗凝剂</div></td>
                <td><div style="width:80px;">备注</div></td>
                <td><div style="width:30px;">签到</div></td>
                @if (Dates.Count > 1)
                {
                    <td><div style="width:30px;">机号</div></td>
                    <td><div style="width:40px;">姓名</div></td>
                    <td><div style="width:60px;">透析器</div></td>
                    <td><div style="width:60px;">路径</div></td>
                    <td><div style="width:60px;">抗凝剂</div></td>
                    <td><div style="width:80px;">备注</div></td>
                    <td><div style="width:30px;">签到</div></td>
                }
            </tr>
            for (int j = 0; j < ((i + 1 < Dates.Count) ? (Dates[i].Count >= Dates[i + 1].Count ? Dates[i].Count : Dates[i + 1].Count) : Dates[i].Count); j++)
            {
                <tr>
                    <td>@(j < Dates[i].Count ? Dates[i][j].MacName : "")</td>
                    <td>@(j < Dates[i].Count ? Dates[i][j].PatName : "")</td>
                    <td>@(j < Dates[i].Count ? Dates[i][j].DialyzerName : "")</td>
                    <td>@(j < Dates[i].Count ? Dates[i][j].RouteName : "")</td>
                    <td>@(j < Dates[i].Count ? Dates[i][j].AnticoagulantName : "")</td>
                    <td>@(j < Dates[i].Count ? Dates[i][j].Remark : "")</td>
                    <td>@(j < Dates[i].Count ? Dates[i][j].SignInSeq.ToString() : "" )</td>
                    @if (i + 1 < Dates.Count)
                    {
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].MacName : "")</td>
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].PatName : "")</td>
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].DialyzerName : "")</td>
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].RouteName : "")</td>
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].AnticoagulantName : "")</td>
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].Remark : "")</td>
                        <td>@(j < Dates[i + 1].Count ? Dates[i + 1][j].SignInSeq.ToString() : "" )</td>
                    }
                    else
                    {
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    }
                </tr>
            }
        }
    </table>
}
